            <tr class="curso">
              <td   id="nombre"><a href="/courses/cursoInicioPublic/<%= rta._id %>"><%= rta.nombre %></a></td>

              <td><%= rta.creador %></td>

            </tr>

        <% });%>
      </table>
      <%}%>


</div></div><div id="pie"><%- include ('partials/footer.ejs') %></div></div>
